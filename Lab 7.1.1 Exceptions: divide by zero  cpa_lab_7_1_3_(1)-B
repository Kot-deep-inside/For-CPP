#include <iostream>

using namespace std;

int main()
{
    int a = 0, b = 0, c = 0;
    cin >> a;
    cin >> b;
    try
    {
        if (b == 0||a==0)
            throw "You cannot divide by zero!";
        c = a / b; 
        cout << c << endl;
    }
    catch (const char* shoto)
    {
        cout << shoto << endl;
    }
   

    return 0;
}
